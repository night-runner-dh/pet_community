<table class="table">
  <thead>
    <tr>
      <th></th>
      <th>グループ名</th>
      <th>メンバー数</>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% groups.each do |group| %>
    <tr>
      <td><%#= image_tag group.get_group_image, size: "50x50" %></td>
      <td><%= link_to group.name, public_group_path(group) %></td>
      <td><%= group.users.count %></td>
    </tr>
    <% end %>
  </tbody>
</table>